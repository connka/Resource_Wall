<!doctype html>
<html lang ="en">
<head>
</head>
<body>
    <% if (!user_id) { %>
        <button type="button" class="button" method="GET" action="/login">
            <input type="submit" value="Login">
        </button>
        <button type="button" class="button" method="GET" action="/register">
            <input type="submit" value="Register">
        </button>
    <% } 
    
    else { %>
    }<form action="/logout" method="POST">
        <label for="user"> <%= user_id %> </label>
        <input type="submit" value="Logout">
    </form>
    <% } %>

</body>
</html>